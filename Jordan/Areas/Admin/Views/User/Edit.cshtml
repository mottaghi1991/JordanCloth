@model Core.Dto.ViewModel.User.AdminUserEditViewModel
@{
    ViewData["Title"] = "Edit";
}
<div class="col-md-12">
    <div class="card card-info">
        <div class="card-header">
            <h3 class="card-title">گروه ها</h3>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-md-12">
                    <form asp-action="Edit" id="myform" method="post">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <input type="hidden" asp-for="ItUserId" />
                        <div class="form-group">
                            <label asp-for="UserName" class="control-label"></label>
                            <input asp-for="UserName" class="form-control" readonly/>
                            <span asp-validation-for="UserName" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="PassWord" class="control-label"></label>
                            <input asp-for="PassWord" type="password" class="form-control" />
                            <span asp-validation-for="PassWord" class="text-danger"></span>
                            
                        </div>
                     

                        <div class="form-group">
                            <input type="submit" value="ثبت" class="btn btn-info" />


                        </div>
                    </form>
                </div>
            </div>
        </div>

    </div>
</div>

